/**app.wxss**/

.container {
  height: 100%;
  /**display: flex;**/
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background-color: #fff;
  font-size: 30rpx;
}

@font-face {
  font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_694935_8p6e3tze1u3.eot?t=1531445110553'); /* IE9*/
  src: url('//at.alicdn.com/t/font_694935_8p6e3tze1u3.eot?t=1531445110553#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('//at.alicdn.com/t/font_694935_8p6e3tze1u3.ttf?t=1531445110553') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+*/ url('//at.alicdn.com/t/font_694935_8p6e3tze1u3.svg?t=1531445110553#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-tianjia-copy:before {
  content: "\e624";
}

.icon-weixin:before {
  content: "\e63f";
}

/*  加载更多   */

.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}

.weui-loadmore_tips {
  display: inline-block;
  vertical-align: middle;
}
